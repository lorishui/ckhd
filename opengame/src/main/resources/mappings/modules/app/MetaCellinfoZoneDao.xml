<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="me.ckhd.opengame.app.dao.MetaCellinfoZoneDao">
	<sql id="MetaCellinfoZoneColumns">
		a.id,
		a.pid,
		a.name,
		a.leaf,
		a.sort
	</sql>

	<!-- 查询全部运营商app信息 -->
	<select id="findAllList" resultType="MetaCellinfoZone">
		SELECT
		<include refid="MetaCellinfoZoneColumns" />
		FROM meta_cellinfo_zone a
		WHERE a.del_flag = 0
		order by sort;
	</select>

</mapper>