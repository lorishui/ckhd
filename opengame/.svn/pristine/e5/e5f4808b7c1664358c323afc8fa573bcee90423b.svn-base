<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="me.ckhd.opengame.app.dao.CfgparamDao">
    <sql id="CfgparamColumns">
    	a.id,
    	a.ckAppId,
    	a.mmAppid,
		a.ckChannelId,
		a.versionName,
		a.province,
		a.carriers,
		a.os,
		a.rqType,
		a.time,
		a.exInfo,
		a.create_by AS "createBy.id",
		a.create_date,
		a.update_by AS "updateBy.id",
		a.update_date,
		a.del_flag,
		a.remarks
    </sql>
	<!-- 根据id获得运营商app信息 -->
	<select id="get" resultType="Cfgparam">
		SELECT
			<include refid="CfgparamColumns"/> 
		FROM app_cfgparam a
		WHERE 1=1
		<if test="ckAppId != null and ckAppId != ''">
			AND a.ckAppId = #{ckAppId}
		</if>
		<if test="mmAppId != null and mmAppId != ''">
			AND a.mmAppId = #{mmAppId} 
		</if>
		<if test="versionName != null and versionName != ''">
			AND a.versionName =#{versionName}
		</if>
		<if test="ckChannelId != null and ckChannelId != ''">
			AND a.ckChannelId =#{ckChannelId}
		</if>
		<if test="id != null and id != ''">
			AND a.id =#{id}
		</if>
	</select>
	
	<!-- 分页查询运营商app信息 -->
	<select id="findList"  resultType="Cfgparam">
		 SELECT
		<include refid="CfgparamColumns" />
		FROM app_cfgparam a
		WHERE a.del_flag = #{delFlag}
		<if test="ckAppId != null and ckAppId != ''">
			AND a.ckAppId = #{ckAppId}
		</if>
		<if test="mmAppId != null and mmAppId != ''">
			AND a.mmAppId = #{mmAppId} 
		</if>
		<if test="versionName != null and versionName != ''">
			AND a.versionName like '%${versionName}%'
		</if>
		<if test="ckChannelId != null and ckChannelId != ''">
			AND a.ckChannelId =#{ckChannelId}
		</if>
		<if test="rqType != null and rqType != ''">
			AND a.rqType =#{rqType}
		</if>
		<choose>
			<when test="page !=null and page.orderBy != null and page.orderBy != ''">
				ORDER BY ${page.orderBy}
			</when>
			<otherwise>
				ORDER BY a.ckappid
			</otherwise>
		</choose>
	</select>
	
	<select id="findAd" resultType="Cfgparam">
		select
		<include refid="CfgparamColumns" />
		FROM app_cfgparam a
		where ckAppId=#{ckAppId} and rqType='ad' and (FIND_IN_SET(#{carriers},carriers) or carriers='#')
		and (FIND_IN_SET(#{mmAppId},mmAppId) or mmAppId='#') and (FIND_IN_SET(#{ckChannelId},ckChannelId) or ckChannelId='#') and (FIND_IN_SET(#{versionName},versionName) or versionName='#') and (FIND_IN_SET(#{province},province) or province='#') order by carriers desc,mmAppId desc,ckChannelId desc,versionName desc,province desc limit 0,1;
	</select>
	
	<select id="findMmextend" resultType="Cfgparam">
		select
		<include refid="CfgparamColumns" />
		FROM app_cfgparam a
		where ckAppId=#{ckAppId} and rqType='mmextend' and (FIND_IN_SET(#{mmAppId},mmAppId) or mmAppId='#') and (FIND_IN_SET(#{ckChannelId},ckChannelId) or ckChannelId='#') and (FIND_IN_SET(#{versionName},versionName) or versionName='#') and (FIND_IN_SET(#{province},province) or province='#') order by mmAppId desc,ckChannelId desc,versionName desc,province desc limit 0,1;
	</select>
	
	<select id="findAndextend" resultType="Cfgparam">
		select
		<include refid="CfgparamColumns" />
