		where ckAppId=#{ckAppId} and rqType='andextend' and (FIND_IN_SET(#{mmAppId},mmAppId) or mmAppId='#') and (FIND_IN_SET(#{ckChannelId},ckChannelId) or ckChannelId='#') and (FIND_IN_SET(#{versionName},versionName) or versionName='#') and (FIND_IN_SET(#{province},province) or province='#') order by mmAppId desc,ckChannelId desc,versionName desc,province desc limit 0,1;
	</select>
	
	<select id="findWoextend" resultType="Cfgparam">
		select
		<include refid="CfgparamColumns" />
		FROM app_cfgparam a
		where ckAppId=#{ckAppId} and rqType='woextend' and (FIND_IN_SET(#{mmAppId},mmAppId) or mmAppId='#') and (FIND_IN_SET(#{ckChannelId},ckChannelId) or ckChannelId='#') and (FIND_IN_SET(#{versionName},versionName) or versionName='#') and (FIND_IN_SET(#{province},province) or province='#') order by mmAppId desc,ckChannelId desc,versionName desc,province desc limit 0,1;
	</select>
	
	<select id="findEgameextend" resultType="Cfgparam">
		select
		<include refid="CfgparamColumns" />
		FROM app_cfgparam a
		where ckAppId=#{ckAppId} and rqType='egameextend' and (FIND_IN_SET(#{mmAppId},mmAppId) or mmAppId='#') and (FIND_IN_SET(#{ckChannelId},ckChannelId) or ckChannelId='#') and (FIND_IN_SET(#{versionName},versionName) or versionName='#') and (FIND_IN_SET(#{province},province) or province='#') order by mmAppId desc,ckChannelId desc,versionName desc,province desc limit 0,1;
	</select>
	
	<!-- mmpaycode del -->
	<select id="findMmpaycode" resultType="Cfgparam">
		select
		<include refid="CfgparamColumns" />
		FROM app_cfgparam a limit 0,1;
	</select>

	<select id="findIosad" resultType="Cfgparam">
		select
		<include refid="CfgparamColumns" />
		FROM app_cfgparam a
		where ckAppId=#{ckAppId} and rqType='iosad' and (FIND_IN_SET(#{versionName},versionName) or versionName='#') order by versionName desc limit 0,1;
	</select>
	
	<select id="findAndroidad" resultType="Cfgparam">
		select
		<include refid="CfgparamColumns" />
		FROM app_cfgparam a
		where ckAppId=#{ckAppId} and rqType='androidad' and (FIND_IN_SET(#{versionName},versionName) or versionName='#') 
		and (FIND_IN_SET(#{province},province) or province='#') and (FIND_IN_SET(#{mmAppId},mmAppId) or mmAppId='#')
		and (FIND_IN_SET(#{carriers},carriers) or carriers='#') and (FIND_IN_SET(#{ckChannelId},ckChannelId) or ckChannelId='#')
		order by carriers desc,mmAppId desc,ckChannelId desc,versionName desc,province desc limit 0,1;
	</select>
	
	<select id="findGamePersonal" resultType="Cfgparam">
		select
		<include refid="CfgparamColumns" />
