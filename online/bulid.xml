<?xml version="1.0" encoding="UTF-8"?>
<project name="online" default="run" basedir=".">
	<property name="src" value="."/>
	<property name="dest" value="classes"/>
	<property name="online_jar" value="online.jar"/>
	<target name="init">
	   <mkdir dir="${dest}"/>
	</target>
	<target name="compile" depends="init">
	   <javac srcdir="${src}" destdir="${dest}" encoding="utf-8"/>
	</target>
	<target name="build" depends="compile">
	   <jar jarfile="${online_jar}" basedir="${dest}"/>
	</target>
	<target name="run" depends="build">
	   <Java classname="me.ckhd.opengame.online.handle.oppoHandle.java" classpath="${hello_jar}"/>
	</target>
	<target name="clean">
	   <delete dir="${dest}" />
	   <delete file="${online_jar}" />
	</target>
	<target name="rerun" depends="clean,run">
	   <ant target="clean" />
	   <ant target="run" />
	</target>
</project>