package me.ckhd.opengame.app.entity;

import me.ckhd.opengame.common.persistence.DataEntity;

public class CfgParamFeilds  extends  DataEntity<CfgParamFeilds>{

	private static final long serialVersionUID = 1L;

	private String ckAppId; // 创酷Appid

	private String type; // 参数所属类型

	private String value; // 参数的key

	private String label; // 参数key对应的中文解释
	
	private String description; // 参数value的描述

	private int classType; // 参数value的类型

	private int sort; // 排序

	private String remarks; // 备注

	public CfgParamFeilds(){}

	public String getCkAppId() {
		return ckAppId;
	}

	public void setCkAppId(String ckAppId) {
		this.ckAppId = ckAppId;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}

	public String getLabel() {
		return label;
	}

	public void setLabel(String label) {
		this.label = label;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public int getClassType() {
		return classType;
	}

	public void setClassType(int classType) {
		this.classType = classType;
	}

	public int getSort() {
		return sort;
	}

	public void setSort(int sort) {
		this.sort = sort;
	}

	public String getRemarks() {
		return remarks;
	}

	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}
	
}
